{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/web/dist/assets/$1"
        },
        {
            "source": "/(.*)\\.(.*)",
            "destination": "/web/dist/$1.$2"
        },
        {
            "source": "/(.*)",
            "destination": "/web/dist/index.html"
        }
    ]
}